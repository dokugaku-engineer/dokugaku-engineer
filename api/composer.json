{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "auth0/login": "^5.3",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^1.0",
        "laravel/cashier": "^12.10",
        "laravel/framework": "^6.20",
        "laravel/tinker": "^2.0",
        "league/csv": "^9.6",
        "league/flysystem-aws-s3-v3": "^1.0",
        "sentry/sentry-laravel": "1.7.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-query-detector": "^1.2",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "mpociot/laravel-apidoc-generator": "^3.17",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/larastan": "^0.6.1",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpstan": [
            "./vendor/bin/phpstan analyse"
        ],
        "phpcs": [
            "./vendor/bin/phpcs -p -s --standard=phpcs.xml --extensions=php ./"
        ],
        "phpcbf": [
            "./vendor/bin/phpcbf --standard=phpcs.xml --extensions=php ./"
        ],
        "phpunit": [
            "./vendor/bin/phpunit"
        ]
    }
}
